{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\SQLEXPRESS;Database=GarageManager-TSC;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "../../log-{Date}.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} - {Message}{NewLine}",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "fileSizeLimitBytes": 10000000,
          "buffered": false,
          "rollOnFileSizeLimit": true,
          "retainedFileCountLimit": 31,
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ]
  
  },
  "AllowedHosts": "*"
}
